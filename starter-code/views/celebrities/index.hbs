<h1>Celebrities</h1>

{{#each celebrities}}
  <p>
    {{this.name}}
  </p>
  <p>
    {{this.occupation}}
  </p>
  <p>
    {{this.catchPhrase}}
  </p>
  <a href="/celebrities/{{this._id}}">
    {{this.name}}
  </a>

  <form action="/celebrities/{{_id}}/delete" method="post">
    <button type="submit">Delete</button>
  </form>
{{/each}}
